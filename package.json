{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vue2-ts-starter",
  "description": "Vue2 TypeScript Startar project for Vite.",
  "version": "1.8.1",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-vue2-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-vue2-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-vue2-ts-starter/issues"
  },
  "engines": {
    "node": ">=18.12.5",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@logue/vue2-helpers": "^2.3.0",
    "patch-package": "^8.0.0",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.5",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/eslint-plugin": "^1.1.7",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/test-utils": "^1.3.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.11.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.29.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-yaml": "^1.0.3",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "neostandard": "^0.11.6",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.79.5",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vitest": "^2.1.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-template-compiler": "^2.7.15",
    "vue-tsc": "^2.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,toml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "postcss": "^8.4.31",
    "semver": "^7.5.3",
    "yaml": "^2.3.2"
  },
  "stackblitz": {
    "startCommand": "yarn run test:unit"
  }
}
